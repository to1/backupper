{
    "name": "to1/backupper",
    "description": "Simple command line based tool to help you back up your project",
    "authors": [
        {
            "name": "Jeton Ahmetaj",
            "email": "jeton.ahmetaj40@gmail.com",
            "homepage": ""
        }
    ],
    "homepage": "https://github.com/to1/backupper",
    "keywords": ["Laravel", "backupper", "backup"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "to1\\backupper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "to1\\backupper\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "to1\\backupper\\backupperServiceProvider"
            ],
            "aliases": {
                "backupper": "to1\\backupper\\Facades\\backupper"
            }
        }
    }
}
